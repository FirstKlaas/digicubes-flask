
<!DOCTYPE html>
<html>
    <head>
        <!--Import Google Icon Font-->
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <!--Import materialize.css-->
        <link type="text/css" rel="stylesheet" href="{{ url_for('static', filename='css/materialize.min.css') }}"  media="screen,projection"/>
        <link type="text/css" rel="stylesheet" href="{{ url_for('static', filename='css/digicubes.css') }}"  media="screen,projection"/>
        {{ moment.include_moment() }}
        <!--Let browser know website is optimized for mobile-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>

    <body>
        <header>
            <div class="navbar-fixed">
            <nav class="top-nav light-blue " role="navigation">

                <div class="container">
                    <div class="nav-wrapper">
                        <span class="brand-logo">DigiCubes {% block space_name %}{% endblock %}</span>
                        <ul class="right hide-on-med-and-down">
                            {% block main_menu %}{% endblock %}
                        </ul>
                    </div>
                </div>
            </nav>
            </div>
        </header>
        <main>
            <div class="navbar-fixed">
            <nav class="light-blue lighten-2 dc-submenu">
            <div class="container">
                <div class="nav-wrapper">
                    <a href="#!" class="brand-logo left">{% block page_title %}{% endblock %}</a>
                    <ul class="right hide-on-med-and-down">
                        {% block sub_menu %}{% endblock %}
                    </ul>
                </div>
            </div>
            </div>
        </nav>
            <div class="container">
                {% block main_content %}{% endblock %}
            </div>      
        </main>

        <!--JavaScript at end of body for optimized loading-->
        <script type="text/javascript" src="{{ url_for('static', filename='js/materialize.min.js') }}"></script>
        <script type="text/javascript" src="{{ url_for('static', filename='js/jquery-3.4.1.slim.min.js') }}"></script>
        <script type="text/javascript" src="{{ url_for('static', filename='js/digicubes.js') }}"></script>
        <script type="text/javascript">
            {% block page_script %}{% endblock %}
        </script>
    </body>
</html>
        